class Solution {
public:
     int check(int i){
         int sum=0;
         while(i){
             int r=i%10;
             sum=sum+r;
             i=i/10;
         }
         return sum;
     }
    int countEven(int num) {

        int sum=0;
     for(int i=1; i<=num; i++){
         if(check(i)%2==0)
           sum++;
           
     }
     
     return sum;
    }
    
};
