class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
       vector<int> v;
       int cnt=0;
        for( auto i=0; i<nums.size(); i++)
        {
            if(nums[i]!=val){
               v.push_back(nums[i]);
            }
           
        }
        nums=v;
        return nums.size();
    }
};
